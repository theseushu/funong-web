const catalogNodes = document.childNodes[0].childNodes.filter((node) => node.tagName === 'div');
const results = catalogNodes.map((catalogNode) => { // class=category-item m
  const result = { groups: [] };
  const catalog = find(
    find(catalogNode.childNodes.filter((n) => n.tagName === 'div')[0].childNodes, (n) => n.tagName === 'h2').childNodes,
    (n) => n.tagName === 'span').childNodes[0].value;
  result.name = catalog;
  const mc = catalogNode.childNodes.filter((n) => n.tagName === 'div')[1];
  const items = mc.childNodes.filter((n) => n.tagName === 'div')[2];
  const list = items.childNodes.filter((node) => node.tagName === 'dl');
  list.forEach((dl) => {
    const dt = find(dl.childNodes, (node) => node.tagName === 'dt');
    const group = find(dt.childNodes, (node) => node.tagName === 'a').childNodes[0].value;
    const dd = find(dl.childNodes, (node) => node.tagName === 'dd');
    const as = dd.childNodes.filter((node) => node.tagName === 'a');
    result.groups.push({
      name: group,
      categories: as.map((a) => a.childNodes[0].value),
    });
  });
  return result;
});


